{
  "name": "m4tlch/laravel-blog",
  "keywords": [
    "laravel",
    "blog",
    "package",
    "admin",
    "panel",
    "rss",
    "feed",
    "write",
    "posts",
    "news",
    "update"
  ],
  "description": "Simple blog (with admin panel) for Laravel based on webdevetc/blogetc",
  "license": "MIT",
  "support": {
    "docs": "https://nikacrm.com/laravel/packages/blogetc-blog-system-for-your-laravel-app/help-documentation/laravel-blog-package-blogetc"
  },
  "authors": [
    {
      "name": "m4tlch",
      "homepage": "https://nikacrm.com/",
      "role": "developer",
      "email": "packages@nikacrm.com"
    }
  ],
  "require": {
    "cviebrock/eloquent-sluggable": "4.5.*|4.6.*",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
    "laravelium/feed": "~2.12.0|~3.0.0",
    "swisnl/laravel-fulltext": "0.13.0",
    "intervention/image": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "M4tlch\\LaravelBlog\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "M4tlch\\LaravelBlog\\M4BlogServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
